F00000381upStatusForTouch = 72
F00000382downStatusForTouch = 73
F00000383menuselected = Val(garage)
F00000384ReverseMenu
F00000385Elseif Menucheck(Val(bovenKamerWork),TX1,TY1) = 1 Then
F00000386upStatusForTouch = 74
